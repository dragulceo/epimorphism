seed_w square {{
	wx = 0.0;
	wy = 0.0;
  if(z.x < @seed_w@ && z.x > -1.0 * @seed_w@ && z.y < @seed_w@ && z.y > -1.0 * @seed_w@){
    w = min((1.0 - abs(z.x) / @seed_w@), (1.0 - abs(z.y) / @seed_w@));
		wx = 4.0 * (z.x + @seed_w@) / (2.0 * @seed_w@);
		wy = 4.0 * (z.y + @seed_w@) / (2.0 * @seed_w@);
  }

  wdt = vec4(w, (w < 0.0 ? 0.0 : 1.0), wx, -1.0 * wy);
}}



id id{{
}}

switch switch{{
	$dim$ v0, v1, init;
	init = $var$;
	%m0%
	v0 = $var$;

	$var$ = init;
	%m1%
	v1 = $var$;

	float x = @intrp@;
	if(x <= 0.0)
		x = 0.0;
	else if(x >= 1.0)
		x = 1.0;

	$var$ = (1.0 - x) * v0 + x * v1;
}}


switch smooth_switch{{
	$dim$ v0, v1, init;

	init = $var$;
	%m0%
	v0 = $var$;

	$var$ = init;
	%m1%
	v1 = $var$;

	float x = @intrp@;
	if(x <= 0.0)
		x = 0.0;
	else if(x >= 1.0)
		x = 1.0;
	else
		x = x * x * (3.0 - 2.0 * x);

	$var$ = (1.0 - x) * v0 + x * v1;
}}




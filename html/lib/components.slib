seed seed_multi {{
	vec4 seed0, seed1, seed2;
	vec2 z_i = z;
  %t%
	%reduce%

	vec2 z_tmp = z;
	%seed0%
	seed0 = seed;

	z = z_tmp;
	%seed1%
	seed1 = seed;

	z = z_tmp;
	%seed2%
	seed2 = seed;

	float a = 1.0 - (1.0 - seed0.a) * (1.0 - seed1.a) * (1.0 - seed2.a);

	if(a == 0.0)
		seed = vec4(0.0, 0.0, 0.0, 0.0);
	else
		seed = 1.0 / a * (seed2.a * seed2 + (1.0 - seed2.a) * seed1.a * seed1 + (1.0 - seed2.a) * (1.0 - seed1.a) * seed0.a * seed0);
	seed.a = a;
	z = z_i;
}}




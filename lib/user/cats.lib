flags {cats, wca}
par {smooth_mask: 50.0}
modules {w:w_lines_border_anim, wt:wt_id, a:alpha_id, seed_color:color1_cats}
scripts [randomize dly:75 spd:0.3 lib:anim dim:vec4 sub:w typ:mod]


}}

seed seed_analysis {{
	vec2 z_i = z;
  %t%
	%reduce%

  float w = VOID_W;
  float wx = (z.x + @w@) / (2.0 * @w@);
  float wy = (z.y + @w@) / (2.0 * @w@);

  if(z.x >= z.y && z.x >= -1.0 * z.y && z.x > (1.0 - @w@)){
    w = (z.x - (1.0 - @w@)) / @w@;
    wy = z.y / (0.5 * @w@) - 0.5;
  }else if(z.y >= z.x && z.y >= -1.0 * z.x && z.y > (1.0 - @w@)){
    w = (z.y - (1.0 - @w@)) / @w@;
    wy = z.x / (0.5 * @w@) - 0.5;
  }else if(z.y >= z.x && z.y <= -1.0 * z.x && z.x < -1.0 * (1.0 - @w@)){
    w = (-1.0 * (1.0 - @w@) - z.x) / @w@;
    wy = z.y / (0.5 * @w@) - 0.5;
  }else if(z.x >= z.y && z.x <= -1.0 * z.y && z.y < -1.0 * (1.0 - @w@)){
    w = (-1.0 * (1.0 - @w@) - z.y) / @w@;
    wy = z.x / (0.5 * @w@) - 0.5;
  }
  wx = w;

  vec4 frame_w;
	if(abs(w-VOID_W) < 0.00001){
		vec2 fr = fract((z + vec2(1,1)) / vec2(2,2));
		seed = vec4(0, fr.x, fr.y, 1.0);
	}else{
	  seed = vec4(w, 0, 0, 1.0);
	}

	z = z_i;
}}


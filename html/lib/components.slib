  wy = z.y / (0.5 * @seed_w@) - 0.5;

  if(z.x > (1.0 - @seed_w@)){
    w = (z.x - (1.0 - @seed_w@)) / @seed_w@;
  }
  else if(z.x < -1.0 * (1.0 - @seed_w@)){
    w = (-1.0 * (1.0 - @seed_w@) - z.x) / @seed_w@;
  }
  wx = w;
  wdt = vec4(w, (w < 0.0 ? 0.0 : 1.0), wx, wy);

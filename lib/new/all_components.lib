	%seed1%
	seed1 = seed;

	%seed2%
	seed2 = seed;

	float a = 1.0 - (1.0 - seed0.a) * (1.0 - seed1.a) * (1.0 - seed2.a);

	if(a == 0.0)
		seed = vec4(0.0, 0.0, 0.0, 0.0);
	else {
		vec4 m1 = mix(seed0.a * seed0, seed1, seed1.a);
		vec4 m2 = mix(m1, seed2, seed2.a);
		seed = 1.0 / a * m2;
	}
	seed.a = a;
	z = z_i;

